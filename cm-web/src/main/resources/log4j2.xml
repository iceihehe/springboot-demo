<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}] [%pid] [%p] - %-100l - %m%n" />
    </Console>
</Appenders>
    <Loggers>
        <logger name="com.iceihehe.cm" level="info">
        </logger>
        <Root level="error">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>